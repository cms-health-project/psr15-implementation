{
    "name": "cms-health-project/psr15-implementation",
    "description": "PSR-15 implementation of the CMS HealthCheck RFC.",
    "type": "library",
    "require": {
        "php": "^8.1",
        "cms-health-project/serializable-reference-implementation": "^0.0.3",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/clock": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.72",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^12.0",
        "doctrine/dbal": "^4.2",
        "spatie/phpunit-snapshot-assertions": "^5.1",
        "symfony/framework-bundle": "^7.2",
        "symfony/clock": "^7.2",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "^2.7",
        "laminas/laminas-diactoros": "^3.5",
        "symfony/psr-http-message-bridge": "^7.2",
        "nyholm/psr7": "^1.8",
        "php-http/discovery": "^1.20",
        "symfony/test-pack": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CmsHealthProject\\Psr15Implementation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CmsHealthProject\\Psr15Implementation\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "php-cs-fixer": "vendor/bin/php-cs-fixer check --diff",
        "phpstan": "vendor/bin/phpstan analyse",
        "phpunit": "vendor/bin/phpunit tests/Unit/"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
